@model List<EPOv2.ViewModels.CapexTransaction>
<script>
    $(function () {
        $("#loading").hide();

        //$(".ui-dialog")
        //        .find(".ui-dialog-titlebar").remove();

        $("#saveManageUser").click(function () {
            //  $("#my-dialog").dialog("close");
            $("#manageUserEdit").hide();

            $(".ui-dialog")
                .css("border", "0 none")
                .css("background", "transparent")
                .find(".ui-dialog-titlebar").remove();
            $("#loading").show();
            // Hide the "busy" Gif at load:
        });
    });
</script>

<div id="loading"><img src="~/Content/img/ajax-loader.gif" alt="" /></div>

   

    
        @*<span>Transactions</span>*@
      
        <table class="table table-striped table-for-modal small">
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Type: @item.User @item.Date @item.Description
                    </td>
                </tr>
            }
        </table>
    
    






